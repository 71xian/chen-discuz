<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.chenyuxian.discuz.system.modular.finance.mapper.FinanceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.chenyuxian.discuz.system.modular.finance.entity.Finance">
        <id column="id" property="id" />
        <result column="created_at" property="createdAt" />
        <result column="income" property="income" />
        <result column="withdrawal" property="withdrawal" />
        <result column="order_count" property="orderCount" />
        <result column="order_amount" property="orderAmount" />
        <result column="total_profit" property="totalProfit" />
        <result column="register_profit" property="registerProfit" />
        <result column="master_portion" property="masterPortion" />
        <result column="withdrawal_profit" property="withdrawalProfit" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        created_at,
        id, income, withdrawal, order_count, order_amount, total_profit, register_profit, master_portion, withdrawal_profit
    </sql>

</mapper>
